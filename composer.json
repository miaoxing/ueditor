{
  "name": "miaoxing/ueditor",
  "type": "miaoxing-plugin",
  "description": "Miaoxing UEditor Plugin",
  "keywords": [
    "miaoxing",
    "ueditor"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.5",
    "miaoxing/file": "^0.3.4"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Ueditor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Ueditor\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "miaoxing/dev": "^8.1.2"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "wei/wei": "twinh/wei as 0.12.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.8.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev",
      "miaoxing/file": "miaoxing/file as 0.3.x-dev"
    }
  }
}
