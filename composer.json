{
  "name": "miaoxing/ueditor",
  "type": "miaoxing-plugin",
  "description": "Miaoxing UEditor Plugin",
  "keywords": [
    "miaoxing",
    "ueditor"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.0",
    "miaoxing/file": "^0.1.9"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Ueditor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Ueditor\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "miaoxing/dev": "^6.4.0"
  },
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev",
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/file": "miaoxing/file as 0.1.x-dev"
    }
  }
}
